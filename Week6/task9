#include <stdlib.h>

int main() {

	using namespace std;
	const int n = 10;
	int arr[n]{ 44, 41, 3, -6, 42, 12, -1, -65, 2, 10 };
	int digits = 0;


		for (int i = 0; i < n; i++) {

			if (arr[i] > 0) {
				digits = digits + 1;
			}


		}

	cout << "Numbers, which is bigger, than 0: " << digits << endl;


	std::getchar();
	std::getchar();

	return 0;
}
